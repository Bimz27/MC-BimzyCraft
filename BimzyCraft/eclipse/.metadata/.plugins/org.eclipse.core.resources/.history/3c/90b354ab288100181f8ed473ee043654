package com.bimzy.bimzycraft.blocks;

import net.minecraft.block.SoundType;
import net.minecraft.block.material.Material;

public class CrusherBlock extends BlockBase 
{
	public CrusherBlock(String name, Material material) 
	{
		super(name, material);
		
		setSoundType(SoundType.METAL);
		setHardness(6.0f);
		setResistance(15.0f);
	}
	
	@Override boolean isFullCube()
	{
		
	}
}
